

<div class="login-box-text"><h1>Log into your account</h1></div>

	<div class="login-box">
		<%= form_for(:session, url: login_path) do |f| %> 
		  <%= f.email_field :email, :placeholder => "Email" %> <br><br>
		  <%= f.password_field :password, :placeholder => "Password" %> <br><br>
		  <%= f.submit "Log in" %>
		<% end %>
	</div>